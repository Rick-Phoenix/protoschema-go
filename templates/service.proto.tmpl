syntax = "proto3";

package {{.PackageName}};

{{range .Imports}}
import "{{ . }}";
{{end}}

{{range .Messages}}
message {{.Name}} {
  {{- if .ReservedNumbers}}
  reserved {{ join .ReservedNumbers ", "}};
  {{- end}}

  {{- range .Fields}}
  {{.Type}} {{.Name}} = {{.Number}}
  {{- if .Options}} [{{.Options}}] {{- end}};
  {{- end}}
}
{{end}}

service {{.Resource}}Service {
  rpc Get{{.Resource}}(Get{{.Resource}}Request) returns(Get{{.Resource}}Response);
}


